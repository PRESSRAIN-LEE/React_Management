{
	"name": "management",
	"version": "1.0.0",
	"scripts": {
		"_client폴더로 이동 후 yarn start로 시작_"
		"client": "cd client && yarn start",
		
		"_server.js을 실행"
		"server": "nodemon server.js",
		
		//개발환경에서는 server와 client를 동시에 실행 (React와 nodejs가 동시에)
		"dev": "concurrently --kill-others-on-fail \"yar server\" \"yarn client\""
	},
	"dependencies": {
		//"body-parser": "1.20.2",
		"body-parser": "^1.18.3",
		//"express": "4.18.2",
		"express": "^4.16.4"
	},
	"devDependencies": {
		//"concurrently": "8.2.0"
		"concurrently": "^4.0.1"
	}
}

